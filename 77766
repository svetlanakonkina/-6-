                    /*  Hide element of slider when using 2-nd lvl of menu  */

                    $('#navmenu-h ul li').mouseover(function() {

                        $('.bx-prev').css('display', 'none');

                    });

                    $('#navmenu-h ul li').mouseleave(function() {

                        $('.bx-prev').css('display', 'block');

                    });

                    $('#navmenu-h ul li').mouseover(function() {

                        $('.bx-next').css('display', 'none');

                    });

                    $('#navmenu-h ul li').mouseleave(function() {

                        $('.bx-next').css('display', 'block');

                    });

